	 <component id="ID_s5_title">
		<text><![CDATA[<b>Man-in-the-Middle Attack Example</b>]]></text>
	</component>
	
	<component id="ID_s5_txt01">
		<text><![CDATA[Victim]]></text>
	</component>
	
	<component id="ID_s5_txt02">
